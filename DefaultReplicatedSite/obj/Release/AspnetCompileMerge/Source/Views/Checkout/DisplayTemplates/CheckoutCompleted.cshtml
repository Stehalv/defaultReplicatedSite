@model CompleteCheckoutStep
@{
    //Todo: need to fix a good thank you page
    var isEnrollment = (Model.Type == CheckoutFlowType.Enrollment || Model.Type == CheckoutFlowType.SimpleEnrollment);
}
    <section id="contact-information">
        <div class="row">
            <div class="col-sm-6">
                @if (isEnrollment)
                {
                    <h3>Enrollment Completed</h3>
                }
                else
                {
                    <h3>Order Completed</h3>
                }
            </div>
        </div>
    </section>